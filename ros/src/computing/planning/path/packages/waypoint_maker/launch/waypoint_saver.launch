<!-- -->
<launch>
	<arg name="save_finename" default="$(find lane_follower)/path/ndtpath.txt" />
	<arg name="save_topic" default="ndt" /> <!--  amcl or ndt or gnss -->

	<node pkg="waypoint_maker" type="waypoint_saver" name="waypoint_saver">
		<param name="save_filename" value="$(arg save_finename)" />
		<param name="save_topic" value="$(arg save_topic)" />
	</node>

</launch>
